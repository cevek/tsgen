{
    "name": "@cevek/tsgen",
    "version": "1.0.2",
    "description": "TypeScript file generator based on EJS templates",
    "main": "dist/bin.js",
    "types": "dist/bin.d.ts",
    "bin": {
        "tsgen": "./dist/bin.js"
    },
    "scripts": {
        "start": "ts-node bin.ts",
        "build": "tsc",
        "prepublishOnly": "npm run build",
