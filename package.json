        "typescript": "^5.3.3"
    },
    "devDependencies": {
        "@types/chokidar": "^1.7.5",
        "@types/commander": "^2.12.2",
        "@types/ejs": "^3.1.5",
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^20.11.19"
    },
    "publishConfig": {
        "access": "public"
