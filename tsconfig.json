{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "lib": ["ES2020"],
        "declaration": true,
        "outDir": "./dist",
        "rootDir": "./",
        "strict": true,
        "jsx": "react",
        "sourceMap": true,
        "declarationMap": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true
    }
}
